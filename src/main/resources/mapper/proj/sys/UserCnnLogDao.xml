<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.comfunny.server.proj.sys.repository.UserCnnLogDao">


    <select id="selectUserCnnLogList" resultType="HMap" >
      SELECT
             USER_ID
           , BIZ_CD
           , DC_CD
           , CLIENT_CD
           , LOGIN_OS
           , LOGIN_IP
           , LANG_CD
           , COUNTRY_CD
           , IN_USER_ID
           , IN_DT
           , UP_USER_ID
           , UP_DT
       FROM tb_sy_user_cnn_log
      WHERE 1=1

    </select>
</mapper>
