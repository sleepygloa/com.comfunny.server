	<queryString>
		<![CDATA[SELECT
*
        FROM    TB_ST_DIST_MANF_M M
            ,   TB_ST_DIST_MANF_D D
            ,   TB_ST_DIST_MANF_W W
            ,   TB_MS_ITEM_M ITEM
            ,   TB_MS_ITEM_UOM_M UOM1
            ,   TB_MS_ITEM_UOM_M UOM2
            ,   TB_MS_CLIENT_M CL
            ,   TB_MS_DC_M DC
            ,   TB_PARAGON_COMM_CODE CC1
            ,   TB_PARAGON_DOMAIN PD
            ,   (
                SELECT T1.ITEM_CD, T1.ITEM_ST_CD, MIN(T1.LOC_CD) LOC_CD
                FROM TB_ST_STOCK_M  T1
                  INNER JOIN TB_MS_LOC_M C
                    ON C.COMPANY_CD   = T1.COMPANY_CD
                    AND C.DC_CD       = T1.DC_CD
                    AND C.LOC_CD      = T1.LOC_CD
                    AND C.HOLD_ST_CD  = 1              --로케이션 보류상태(1:정상, 2:보류)
                    AND C.LOC_TYPE_CD IN (SELECT CODE_CD FROM TB_PARAGON_COMM_CODE WHERE COMPANY_CD = T1.COMPANY_CD AND CODE_GROUP_CD = 'LOC_TYPE_CD' AND CODE_OTHER2 = '1') --보관로케이션 유형 (1.보관, 2.출고, 4.불량)
                    AND C.USE_YN      = 'Y'
                WHERE STOCK_QTY > 0
                AND   STOCK_QTY +IB_PLAN_QTY-(OB_PLAN_QTY-HOLD_QTY) > 0
                GROUP BY T1.ITEM_CD, T1.ITEM_ST_CD
                ) LOC
       WHERE    M.COMPANY_CD            = D.COMPANY_CD
         AND    M.DIST_MANF_NO          = D.DIST_MANF_NO
         AND    M.COMPANY_CD            = W.COMPANY_CD(+)
         AND    M.DIST_MANF_NO          = W.DIST_MANF_NO(+)
         AND    D.DIST_MANF_DETAIL_SEQ  = W.DIST_MANF_DETAIL_SEQ(+)
         AND    M.COMPANY_CD            = ITEM.COMPANY_CD
         AND    M.CLIENT_CD             = ITEM.CLIENT_CD
         AND    D.ITEM_CD               = ITEM.ITEM_CD
         AND    M.COMPANY_CD            = CL.COMPANY_CD
         AND    M.CLIENT_CD             = CL.CLIENT_CD
         AND    M.COMPANY_CD            = DC.COMPANY_CD
         AND    M.DC_CD                 = DC.DC_CD
         AND    M.COMPANY_CD            = UOM1.COMPANY_CD
         AND    M.CLIENT_CD             = UOM1.CLIENT_CD
         AND    D.ITEM_CD               = UOM1.ITEM_CD
         AND    M.COMPANY_CD            = UOM2.COMPANY_CD
         AND    M.CLIENT_CD             = UOM2.CLIENT_CD
         AND    D.ITEM_CD               = UOM2.ITEM_CD
         AND    D.COMPANY_CD            = CC1.COMPANY_CD(+)
         AND    D.ITEM_ST_CD            = CC1.CODE_CD(+)
         AND    CC1.CODE_GROUP_CD(+)    = 'ITEM_ST_CD'
         AND    (CC1.CODE_GROUP_CD||CC1.CODE_CD) = PD.DOMAIN_ID(+)
         AND    PD.LANG_CD(+)           = $P{LANG_CD}
         AND    M.COMPANY_CD            = CC1.COMPANY_CD
         AND    M.COMPANY_CD            = PD.COMPANY_CD(+)
         AND    UOM1.LOAD_GBN_CD        = '2'
         AND    UOM2.LOAD_GBN_CD        = '3'
         AND    M.USE_YN                = 'Y'
         AND    D.USE_YN                = 'Y'
         AND    W.USE_YN(+)             = 'Y'
         AND    M.DIST_MANF_GBN_CD      = '2'
         AND    D.IOB_GBN_CD            = '2'
         AND    LOC.ITEM_CD             = D.ITEM_CD
         AND    LOC.ITEM_ST_CD          = D.ITEM_ST_CD
         AND    M.COMPANY_CD            = $P{COMPANY_CD}
         AND    M.CLIENT_CD             = $P{CLIENT_CD}
         AND    M.DC_CD                 = $P{DC_CD}
         AND    M.DIST_MANF_NO          = $P{DIST_MANF_NO}
    ORDER BY M.DIST_MANF_NO, W.DIST_MANF_INST_NO]]>
	</queryString>
