	<queryString>
		<![CDATA[SELECT /* [DistributionManufactureService.query][listStManuDist][유통가공처리 세트제품목록][Kim Seon Ho][2019-01-08] */
                M.COMPANY_CD
            ,   M.DC_CD
            ,   DC.DC_NM
            ,   M.CLIENT_CD
            ,   CL.CLIENT_NM
            ,   M.DIST_MANF_NO
            ,   $P{USER_NM}  AS USER_NM
            ,   TO_CHAR(TO_DATE(M.WORK_YMD, 'YYYYMMDD'), 'YYYY-MM-DD')   AS WORK_YMD
            ,   M.DIST_MANF_GBN_CD
            ,   D.IOB_GBN_CD
            ,   TO_CHAR(D.WORK_DT,'YYYY-MM-DD HH24:MI:SS')       AS WORK_DT
        FROM    TB_ST_DIST_MANF_M M
            ,   TB_ST_DIST_MANF_D D
            ,   TB_ST_DIST_MANF_W W
            ,   TB_MS_CLIENT_M CL
            ,   TB_MS_DC_M DC
       WHERE    M.COMPANY_CD            = D.COMPANY_CD
         AND    M.DIST_MANF_NO          = D.DIST_MANF_NO
         AND    M.COMPANY_CD            = W.COMPANY_CD(+)
         AND    M.DIST_MANF_NO          = W.DIST_MANF_NO(+)
         AND    D.DIST_MANF_DETAIL_SEQ  = W.DIST_MANF_DETAIL_SEQ(+)
         AND    M.COMPANY_CD            = CL.COMPANY_CD
         AND    M.CLIENT_CD             = CL.CLIENT_CD
         AND    M.COMPANY_CD            = DC.COMPANY_CD
         AND    M.DC_CD                 = DC.DC_CD
         AND    M.USE_YN                = 'Y'
         AND    D.USE_YN                = 'Y'
         AND    W.USE_YN(+)             = 'Y'
         AND    M.COMPANY_CD            = $P{COMPANY_CD}
         AND    M.CLIENT_CD             = $P{CLIENT_CD}
         AND    M.DC_CD                 = $P{DC_CD}
         AND    M.DIST_MANF_GBN_CD      = $P{DIST_MANF_GBN_CD}
         AND    D.IOB_GBN_CD            = $P{IOB_GBN_CD}
         AND    M.DIST_MANF_NO          =$P{DIST_MANF_NO}
    ORDER BY M.DIST_MANF_NO, M.WORK_YMD, W.DIST_MANF_INST_NO]]>
	</queryString>
