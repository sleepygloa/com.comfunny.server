	<queryString>
		<![CDATA[SELECT * FROM
(SELECT  trunc(OL.REPORT_SEQ/ 11) + 1 as PG
      , ROUND(COUNT(*) OVER (PARTITION BY NVL(OB.SONO, OL.SO_NO))/10 , 0) AS TPG
      , OL.REPORT_SEQ
      , NVL(OB.RINO, OL.RI_NO) AS RINO
      , NVL(OB.SONO, OL.SO_NO) AS SONO
      , OB.CLIENTCD
      , OB.YMD
      , OB.USERNM
      , OB.STORECD
      , OB.STORENM
      , OB.BIZNO
      , OB.TELNO
      , OB.BASICADDR
      , OB.DETAILADDR
      , OB.RSTORECD
      , OB.RSTORENM
      , OB.RBIZNO
      , OB.RTELNO
      , OB.RBASICADDR
      , OB.RDETAILADDR
      , OB.ITEMCD
      , OB.ITEMNM
      , OB.UOM
      , OB.PKQTY
      , OB.OBCOST
      , OB.AMOUNT
      , OB.REMARK
      , OB.CARNO
      , OB.SOYMD
      , COUNT(*) OVER (PARTITION BY NVL(OB.SONO, OL.SO_NO)) AS CN
      --, OB.RECVRNM
  FROM  (
        SELECT BB.REPORT_SEQ, NULL AS DT, RR.SO_NO, RR.RI_NO
          FROM
              (
                SELECT CASE WHEN (MAX(AA.LINE) / 10) >= 1 THEN (TRUNC((MAX(AA.LINE) / 10 )) * 10) +
                            CASE WHEN MOD(MAX(AA.LINE) , 10) > 0 THEN 10
                            ELSE 0
                            END
                       ELSE 9
                       END AS REPORT_LINE,
                       AA.SO_NO,
                       AA.RI_NO
                  FROM (
                        SELECT MAX(D.RI_DETAIL_SEQ)                         AS LINE
                            ,  M.SO_NO
                            ,  M.RI_NO
                          FROM TB_RI_M M
                            ,  TB_RI_D D
                            ,  TB_MS_ITEM_M ITEM
                            ,  TB_MS_ITEM_UOM_M UOM
                          WHERE M.COMPANY_CD    = D.COMPANY_CD
                            AND M.RI_NO         = D.RI_NO
                            AND M.RI_PROG_ST_CD = D.RI_PROG_ST_CD
                            AND M.COMPANY_CD    = $P{s_companyCd}
                            AND M.CLIENT_CD     = $P{clientCd}
                            AND M.DC_CD         = $P{s_dcCd_Prioord}
                            AND M.RI_NO         = $P{riNo}
                            AND M.USE_YN        = 'Y'
                            AND D.USE_YN        = 'Y'
                        GROUP BY SO_NO, M.RI_NO
                        ) AA
                GROUP BY AA.SO_NO, AA.RI_NO
              ) RR
              JOIN TB_REPORT_M BB ON BB.REPORT_SEQ <= RR.REPORT_LINE
       ) OL
        LEFT JOIN (
                           SELECT M.RI_NO                                   AS RINO
                            ,   D.RI_DETAIL_SEQ                             AS DETAILSEQ
                            ,   M.SO_NO                                     AS SONO
                            ,   M.CLIENT_CD                                 AS CLIENTCD
                            ,   SYSDATE                                     AS YMD
                            , (SELECT USER_NM
                                 FROM TB_PARAGON_USER_INFO
                                WHERE USER_ID = $P{s_userId}
                                  AND USE_YN = 'Y'
                                )                                           AS USERNM
                            ,   M.STORE_CD                                  AS STORECD
                            ,   ST1.STORE_NM                                AS STORENM
                            ,   ST1.BIZ_NO	                            AS BIZNO
                            ,   ST1.TEL_NO                                  AS TELNO
                            ,   ST1.BASIC_ADDR                              AS BASICADDR
                            ,   ST1.DETAIL_ADDR                             AS DETAILADDR
                            ,   M.RSTORE_CD                                 AS RSTORECD
                            ,   ST2.STORE_NM                                AS RSTORENM
                            ,   ST2.BIZ_NO                                  AS RBIZNO
                            ,   ST2.TEL_NO                                  AS RTELNO
                            ,   ST2.BASIC_ADDR                              AS RBASICADDR
                            ,   ST2.DETAIL_ADDR                             AS RDETAILADDR
                            ,   D.ITEM_CD                                   AS ITEMCD
                            ,   ITEM.ITEM_NM                                AS ITEMNM
                            ,   (UOM.STD_UOM_CD ||'/'|| UOM.CONV_UOM_CD)    AS UOM
                            ,   UOM.CONV_UOM_QTY                            AS PKQTY
                            ,   CASE WHEN ST1.AMT_PRT_YN = 'Y' THEN ITEM.OB_COST ELSE 0 END   AS OBCOST
                            ,   CASE WHEN ST1.AMT_PRT_YN = 'Y' THEN UOM.CONV_UOM_QTY * ITEM.OB_COST ELSE 0 END   AS AMOUNT
                            ,   M.REMARK                                    AS REMARK
                            ,   M.CAR_NO                                    AS CARNO
                            ,   M.SO_YMD                                    AS SOYMD
--                            ,   M.RECVR_NM                                  AS RECVRNM
                          FROM  TB_RI_M M
                            ,   TB_RI_D D
                            ,   TB_MS_STORE_M ST1
                            ,   TB_MS_STORE_M ST2
                            ,   TB_MS_ITEM_M ITEM
                            ,   TB_MS_ITEM_UOM_M UOM
                         WHERE  M.COMPANY_CD        = D.COMPANY_CD
                            AND M.RI_NO             = D.RI_NO
                            AND M.RI_PROG_ST_CD     = D.RI_PROG_ST_CD
                            AND M.COMPANY_CD        = ST1.COMPANY_CD
                            AND M.CLIENT_CD         = ST1.CLIENT_CD
                            AND M.STORE_CD          = ST1.STORE_CD
                            AND M.COMPANY_CD        = ST2.COMPANY_CD
                            AND M.CLIENT_CD         = ST2.CLIENT_CD
                            AND M.RSTORE_CD         = ST2.STORE_CD
                            AND M.COMPANY_CD        = ITEM.COMPANY_CD
                            AND M.CLIENT_CD         = ITEM.CLIENT_CD
                            AND D.ITEM_CD           = ITEM.ITEM_CD
                            AND M.COMPANY_CD        = UOM.COMPANY_CD
                            AND M.CLIENT_CD         = UOM.CLIENT_CD
                            AND D.ITEM_CD           = UOM.ITEM_CD
                            AND UOM.LOAD_GBN_CD     = '2'
                            AND M.COMPANY_CD        = $P{s_companyCd}
                            AND M.DC_CD             = $P{s_dcCd_Prioord}
                            AND M.CLIENT_CD         = $P{clientCd}
                            AND M.RI_NO             = $P{riNo}
                            AND M.USE_YN            = 'Y'
                    ) OB ON OB.DETAILSEQ = OL.REPORT_SEQ
ORDER BY OL.REPORT_SEQ)]]>
	</queryString>
