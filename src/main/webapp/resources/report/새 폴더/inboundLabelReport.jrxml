	<parameter name="CLIENTCD" class="java.lang.String">
		<parameterDescription><![CDATA[고객사]]></parameterDescription>
	</parameter>
	<parameter name="DCCD" class="java.lang.String">
		<parameterDescription><![CDATA[물류센터]]></parameterDescription>
	</parameter>
	<parameter name="COMPANYCD" class="java.lang.String"/>
	<parameter name="IBNO" class="java.lang.String"/>
	<parameter name="IBDETAILSEQ" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT /* [inboundInstruction.query][getInboundLabelReportList][입고라벨표 레포트 조회][Kim Seon Ho][2017-09-16] */
               M.COMPANY_CD                      AS COMPANYCD    -- 회사번호
             , DC.DC_NM                          AS DCNM         -- 물류센터
             , CL.CLIENT_NM                      AS CLIENTNM     -- 고객사이름
             , SUP.SUPPLIER_NM                   AS SUPNM        -- 공급처
             , M.PO_NO                           AS PONO         -- 발주번호
             , M.IB_NO                           AS IBNO --입고번호
             , TO_CHAR(TO_DATE(M.IB_YMD, 'YYYYMMDD'), 'YYYY-MM-DD') AS IBYMD --입고일자
             , D.ITEM_CD                                            AS ITEMCD        --제품코드
             , ITEM.ITEM_NM                                         AS ITEMNM     --제품명
             , ITEM.ITEM_SPEC                                       AS ITEMSPEC   --제품규격
             , CD1.CODE_NM                                          AS ITEMSTNM
             , TO_CHAR(TO_DATE(D.DIST_EXPIRY_YMD, 'YYYYMMDD'), 'YYYY-MM-DD') AS DISTEXPIRYYMD --유통기한일수
             , D.MAKE_LOT                                                    AS MAKELOT --제조LOT
             , TO_CHAR(TO_DATE(D.MAKE_YMD, 'YYYYMMDD'), 'YYYY-MM-DD')        AS MAKEYMD --제조일자
             , UOM.CONV_UOM_QTY                                              AS PKQTY --입수
             , UOM.STD_UOM_CD ||'/'|| UOM.CONV_UOM_CD                        AS UOM  --단위
             , D.IB_DETAIL_SEQ                                               AS IBDETAILSEQ --입고순번
             , NVL(INST.INST_QTY, 0)                                            AS INSTTOTQTY  --입고총예정수량도메인명처리
             , NVL(FLOOR(INST.INST_QTY / UOM.CONV_UOM_QTY ), 0)                 AS INSTBOXQTY  --박스
             , NVL(MOD(INST.INST_QTY, UOM.CONV_UOM_QTY ), 0 )                   AS INSTEAQTY      --낱개
             , TO_CHAR(sysdate, 'YYYY-MM-DD HH24:MI:SS')                     AS YMD --출력일자
             , INST.PLT_ID                                                   AS PLTID --파레트ID
             , INST.INST_LOC_CD                                              AS INSTLOCCD --지시로케이션 코드
          FROM TB_IB_M M
             , TB_IB_D D
             , TB_IB_INST_M INST
             , TB_MS_ITEM_M ITEM
             , TB_MS_ITEM_UOM_M UOM
             , TB_MS_SUPPLIER_M SUP
             , TB_MS_DC_M DC
             , TB_MS_CLIENT_M CL
             , (SELECT COMPANY_CD, CODE_GROUP_CD, CODE_CD, CODE_NM FROM TB_PARAGON_COMM_CODE) CD1
         WHERE M.COMPANY_CD    = D.COMPANY_CD
           AND M.IB_NO         = D.IB_NO
           AND M.IB_PROG_ST_CD = D.IB_PROG_ST_CD
           AND M.COMPANY_CD    = INST.COMPANY_CD
           AND M.IB_NO         = INST.IB_NO
           AND D.IB_DETAIL_SEQ = INST.IB_DETAIL_SEQ
           AND M.COMPANY_CD    = ITEM.COMPANY_CD
           AND M.CLIENT_CD     = ITEM.CLIENT_CD
           AND D.ITEM_CD       = ITEM.ITEM_CD
           AND M.COMPANY_CD    = UOM.COMPANY_CD
           AND M.CLIENT_CD     = UOM.CLIENT_CD
           AND D.ITEM_CD       = UOM.ITEM_CD
           AND M.COMPANY_CD    = SUP.COMPANY_CD
           AND M.CLIENT_CD     = SUP.CLIENT_CD
           AND M.SUPPLIER_CD   = SUP.SUPPLIER_CD
           AND M.COMPANY_CD    = DC.COMPANY_CD
           AND M.DC_CD         = DC.DC_CD
           AND M.COMPANY_CD    = CL.COMPANY_CD
           AND M.CLIENT_CD     = CL.CLIENT_CD
           AND D.COMPANY_CD    = CD1.COMPANY_CD(+)
           AND D.ITEM_ST_CD    = CD1.CODE_CD(+)
           AND CD1.CODE_GROUP_CD(+) = 'ITEM_ST_CD'
           AND M.USE_YN        = 'Y'
           AND D.USE_YN        = 'Y'
           AND ITEM.USE_YN     = 'Y'
           AND UOM.USE_YN      = 'Y'
           AND DC.USE_YN       = 'Y'
           AND SUP.USE_YN      = 'Y'
           AND INST.USE_YN     = 'Y'
           AND M.IB_PROG_ST_CD = '40'
           AND UOM.LOAD_GBN_CD    = '2'
           AND M.COMPANY_CD = $P{COMPANYCD}
           AND M.CLIENT_CD  = $P{CLIENTCD}
           AND M.DC_CD      = $P{DCCD}
           AND M.IB_NO = $P{IBNO}
           AND D.IB_DETAIL_SEQ = $P{IBDETAILSEQ}
           ORDER BY M.IB_YMD, M.IB_NO, D.IB_DETAIL_SEQ ASC]]>
	</queryString>
